#include<stdio.h>
int main()
{
    int t;
    scanf("%d",&t);
    for(int p = 0;p<t;p++)
    {
        int x,a,n,number[100],f=0,sum=0,ok=0;
        scanf("%d %d",&n,&x);
        for(int i = 0;i<n;i++)
        {
            scanf("%d",&number[i]);
        }
        for (int i = 0; i < n; i++)
		{
			for (int j = i + 1; j < n; j++)
			{
				if (number[i] < number[j])
				{
					a = number[i];
					number[i] = number[j];
					number[j] = a;
				}
			}
		}
		for(int i = 0;i<n;i++)
		{
		    sum+=number[i];
		    f++;
		    if(sum>=x)
		    {
		        ok = 1;
		        break;
		    }
		}
		    if(ok==1)
		    {
		        printf("%d",f);
		    }
		    else if(ok==0)
		    {
		    printf("-1");
		    }
		
		printf("\n");
    }
    return 0;
}
